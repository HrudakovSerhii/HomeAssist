{
  "name": "@home-assist/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "frontend:dev": "npx dotenv -e .env.development -- npx nx serve admin.client",
    "frontend:build": "npx dotenv -e .env.test -- npx nx build admin.client",
    "frontend:test": "npx dotenv -e .env.development -- npx nx test admin.client",
    "backend:dev": "npx dotenv -e .env.development -- npx nx serve backend.root",
    "backend:build": "npx dotenv -e .env.test -- npx nx build backend.root",
    "backend:build:prod": "npx dotenv -e .env.production -- npx nx build backend.root --configuration=production",
    "backend:test": "npx dotenv -e .env.development -- npx nx test backend.root",
    "db:migrate": "cd apps/backend.root && npx dotenv -e ../../.env.development -- npx prisma migrate dev",
    "db:generate": "cd apps/backend.root && npx dotenv -e ../../.env.development -- npx prisma generate",
    "db:studio": "cd apps/backend.root && npx dotenv -e ../../.env.development -- npx prisma studio",
    "db:docker:up": "docker compose -f ./docker-compose.db.yml up",
    "generate-types": "cd libs/api-types && npm run generate-types"
  },
  "private": true,
  "devDependencies": {
